<h1>New circule</h1>

<% form_for(@circule) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :phone_number %><br />
    <%= f.text_field :phone_number %>
  </p>
  <p>
    <%= f.label :start_at %><br />
    <%= f.date_select :start_at %>
  </p>
  <p>
    <%= f.label :end_at %><br />
    <%= f.date_select :end_at %>
  </p>
  <p>
    <%= f.label :create_at %><br />
    <%= f.date_select :create_at %>
  </p>
  <p>
    <%= f.label :flickr_set_url %><br />
    <%= f.text_field :flickr_set_url %>
  </p>
  <p>
    <%= f.label :references %><br />
    <%= f.text_field :references %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', circules_path %>
