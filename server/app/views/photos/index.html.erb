<h1>Listing photos</h1>

<table>
  <tr>
    <th>Taken time</th>
    <th>Thumbnail url</th>
    <th>Url</th>
    <th>Site</th>
  </tr>

<% for photo in @photos %>
  <tr>
    <td><%=h photo.taken_time %></td>
    <td><%=h photo.thumbnail_url %></td>
    <td><%=h photo.url %></td>
    <td><%=h photo.site_id %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New photo', new_photo_path %>
